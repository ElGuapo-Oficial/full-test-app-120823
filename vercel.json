{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
