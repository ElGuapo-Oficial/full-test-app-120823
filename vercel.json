{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/dist/public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}
