{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "s-maxage=31536000, immutable" },
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
  "includeFiles": ["public/**/*"]
}
